@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>角色的权限</title>
</head>
<body>
    <table>
        <tr>
            <td>RoleId</td>
            <td>角色名字</td>
            <td>权限名字</td>
            <td>权限描述</td>
            <td>控制器名字</td>
            <td>action名字</td>
        </tr>
        @{
            foreach (var entity in Model)
            {
                <tr>
                    <td>@Model.Id</td>
                    <td>@Model.Name</td>
                    <td>@entity.ModuleName</td>
                    <td>@entity.ModuleDescribe</td>
                    <td>@entity.ControllerName</td>
                    <td>@entity.ActionName</td>
                </tr>
            }
        }
    </table>
</body>
</html>