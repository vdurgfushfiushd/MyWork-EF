@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>用户角色</title>
</head>
<body>
    <table>
        <tr>
            <td>用户id</td>
            <td>用户名字</td>
            <td>用户密码</td>
            <td>用户身高</td>
            <td>用户描述</td>
            <td>角色名字</td>
            <td>角色描述</td>
        </tr>
        @{
            foreach (var userrole in Model)
            {
                <tr>
                    <td>@userrole.Id</td>
                    <td>@userrole.Name</td>
                    <td>@userrole.Password</td>
                    <td>@userrole.Height</td>
                    <td>@userrole.Describe</td>
                    <td>@userrole.RoleName</td>
                    <td>@userrole.RoleDescribe</td>
                </tr>
            }
        }
    </table>
</body>
</html>



